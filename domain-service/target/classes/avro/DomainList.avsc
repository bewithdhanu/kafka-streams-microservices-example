{
  "type": "record",
  "name": "DomainList",
  "namespace": "com.example.schema",
  "fields": [
    {
      "name": "domains",
      "type": {
        "type": "array",
        "items": {
          "type": "record",
          "name": "Domain",
          "fields": [
            {"name": "domain", "type": ["null", "string"]},
            {"name": "create_date", "type": ["null", "string"]},
            {"name": "update_date", "type": ["null", "string"]},
            {"name": "country", "type": ["null", "string"]},
            {"name": "isDead", "type": "boolean"},
            {"name": "A", "type": ["null", {"type": "array", "items": "string"}], "default": null},
            {"name": "NS", "type": ["null", {"type": "array", "items": "string"}], "default": null},
            {"name": "CNAME", "type": ["null", "string"], "default": null},
            {"name": "MX", "type": ["null", {"type": "array", "items": {
              "type": "record",
              "name": "MXRecord",
              "fields": [
                {"name": "exchange", "type": ["null", "string"]},
                {"name": "priority", "type": "int"}
              ]
            }}], "default": null},
            {"name": "TXT", "type": ["null", {"type": "array", "items": "string"}], "default": null}
          ]
        }
      }
    },
    {"name": "total", "type": "int"},
    {"name": "time", "type": ["null", "string"]},
    {"name": "next_page", "type": ["null", "string"], "default": null}
  ]
}
